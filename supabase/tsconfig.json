{
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["esnext", "dom"],
    "types": ["@supabase/supabase-js"],
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "checkJs": false,
    "allowJs": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
} 